      <th><input form="f0" id=to type="text" name="ot" placeholder="Вих.ТГ" value="{{.Old.Ot}}"/></th>
      <th><input form="f0" id=ud type="text" name="du" placeholder="Сек." value="{{.Old.Du}}"/></th>
    </tr>
    </thead>
  </table>
<div class="temp">
